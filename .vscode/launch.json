            "cwd": "${workspaceFolder}/build/satacc/simple_test/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) minisat",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/minisat/minisat",
            "args": [
                "-enable-acc",
