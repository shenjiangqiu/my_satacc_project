    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) main_test intersim",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/satacc/main_test",
            "args": [
                "intersim"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) main_test acc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/satacc/main_test",
            "args": [
                "-e",
                "acc"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) main_test simple acc",
            "type": "cppdbg",
            "request": "launch",
